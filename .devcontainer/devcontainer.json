{
	"name": "Course Search",
	"image": "mcr.microsoft.com/devcontainers/python:0-3.11",
	"workspaceFolder": "/workspaces/course-search",

	"forwardPorts": [8000],
    "portsAttributes": {
        "8000": {
            "label": "Server",
            "onAutoForward": "silent"
        }
    },

	"postCreateCommand": "pip3 install -r requirements.txt"
}